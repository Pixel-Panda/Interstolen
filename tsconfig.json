{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": [
      "@astrojs/image/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@css/*": [
        "src/styles/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@typings/*": [
        "src/types/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ]
    }
  }
}